class Shape {
    double getArea() {
        return 0.0;
    }
}

class Rectangle extends Shape {
    double getArea() {
        // Assuming width and height are defined somewhere
        double width = 5.0;
        double height = 3.0;
        return width * height;
    }
}

public class ShapeTest {
    public static void main(String[] args) {
        Shape shape = new Shape();
        System.out.println("Area of shape: " + shape.getArea()); // Outputs: 0.0
        
        Rectangle rectangle = new Rectangle();
        System.out.println("Area of rectangle: " + rectangle.getArea()); // Outputs: 15.0
    }
}
